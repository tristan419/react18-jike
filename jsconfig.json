{
    "compilerOptions": {
      "baseUrl": "./",
      "paths": {
        "@/*": ["src/*"]
      },
      "jsx": "react-jsx",
      "module": "ESNext",
      "moduleResolution": "Node",
      "target": "ES2020",
      "esModuleInterop": true,
      "strict": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": [
      "src"
    ]
  }